<% dispute = DisputePresenter.new(dispute: @dispute, view: self) %>
<% domain = DomainPresenter.new(domain: @dispute.domain, view: self) %>

<ol class="breadcrumb">
    <li><%= link_to t('admin.disputes.index.title'), admin_disputes_path %></li>
</ol>

<div class="page-header">
    <h1><%= dispute.name %></h1>
</div>

<div class="col-sm-6">
    <%= render 'overview', dispute: dispute, domain: domain %>
</div>
